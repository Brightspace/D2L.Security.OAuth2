<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net48</TargetFramework>
		<LangVersion>7.3</LangVersion>
		<OutputType>Library</OutputType>

		<Title>D2L.Security.OAuth2</Title>
		<Description>Library for interacting with OAuth2 based flows in D2L systems</Description>
		<Product>D2L.Security.OAuth2</Product>
		<Version>0.0.0</Version>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/Brightspace/D2L.Security.OAuth2</PackageProjectUrl>
		<Authors>D2L</Authors>
		<Company>D2L Corporation</Company>
		<Copyright>Copyright © D2L Corporation 2018</Copyright>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="D2L.CodeStyle.Analyzers" Version="0.130.0">
			<PrivateAssets>All</PrivateAssets>
		</PackageReference>
		<PackageReference Include="D2L.CodeStyle.Annotations" Version="0.23.0" />
		<PackageReference Include="D2L.Services.Core.Exceptions" Version="2.0.1.15" />
		<PackageReference Include="D2L.Services.Core.Extensions" Version="2.0.3.46" />
		<PackageReference Include="Newtonsoft.Json" Version="6.0.6" />
		<PackageReference Include="System.Collections.Immutable" Version="1.5.0" />
		<PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="4.0.1" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="System.IdentityModel" />
		<Reference Include="System.Net.Http" />
		<Reference Include="System.Runtime.Caching" />
		<Reference Include="System.Web" />
	</ItemGroup>

</Project>
